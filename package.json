    "serve": "vite preview",
    "start": "vite",
    "test": "vitest run"
  },
  "dependencies": {
    "solid-js": "1.8.17"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@release-it/conventional-changelog": "8.0.1",
    "@typescript-eslint/eslint-plugin": "7.9.0",
    "@typescript-eslint/parser": "7.9.0",
    "@vitest/coverage-v8": "1.6.0",
    "eslint": "8.57.0",
    "husky": "9.0.11",
    "jsdom": "24.0.0",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "release-it": "17.2.1",
    "tsx": "4.10.5",
    "typescript": "5.4.5",
    "vite": "5.2.11",
    "vite-plugin-solid": "2.10.2",
    "vitest": "1.6.0"
  }
